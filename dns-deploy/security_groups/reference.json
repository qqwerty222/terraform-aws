    "vpc_id":"data.consul_keys.network.var.vpc_id",
    
    "ingress":[
      { "to_port":22, "from_port":0, "protocol":"tcp", "cidr_blocks":["1.1.1.1/32"]}
    ],
    
    "egress":[
      {"to_port":0, "from_port":0, "protocol":"-1", "cidr_blocks":["0.0.0.0/0"]}
